{
  "name": "Analogue",
  "short_name": "Analogue",
  "version": "1.0.1",
  "description": "Share what inspires you.",
  "manifest_version": 2,
  "key": "",
  "minimum_chrome_version": "79",
  "homepage_url": "https://www.analogue.app",
  "permissions": ["activeTab", "contextMenus", "notifications", "http://localhost/*", "<all_urls>"],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.getstream.io; object-src 'self';",
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
  "externally_connectable": {
    "matches": ["http://localhost/*"]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/extension-icon-x16.png",
      "32": "icons/extension-icon-x32.png",
      "48": "icons/extension-icon-x48.png",
      "128": "icons/extension-icon-x128.png"
    },
    "default_title": "Add to Analogue"
  },
  "commands": {
        "_execute_browser_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+Y",
            "mac": "Command+Shift+Y"
            }
      }
  },
  "icons": {
    "16": "icons/extension-icon-x16.png",
    "32": "icons/extension-icon-x32.png",
    "48": "icons/extension-icon-x48.png",
    "128": "icons/extension-icon-x128.png"
  },
  "web_accessible_resources": [
    "css/all.css"
  ]
}
